{
  "presets": ["@babel/env", "@babel/react"],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    [
      "module-resolver",
      {
        "alias": {
          "@components": "./src/components",
          "@pages": "./src/pages",
          "@assets": "./src/assets",
          "@hooks": "./src/hooks",
          "@commons": "./src/commons"
        }
      }
    ]
  ]
}
